{
  "defaultLocale": "en",
  "files": {
    "gt": {
      "output": "public/_gt/[locale].json"
    },
    "json": {
      "include": ["content/ui.[locale].json"]
    },
    "mdx": {
      "include": ["content/docs/[locale]/**/*.mdx"],
      "exclude": ["content/docs/[locale]/changelog/**/*.mdx"]
    }
  },
  "locales": ["zh", "de", "fr", "es", "ja"]
}
