    "fumadocs-core": "^15.1.2",
    "fumadocs-mdx": "^11.5.7",
    "fumadocs-ui": "^15.1.2",
    "gt-next": "^5.2.24-alpha.1",
    "gtx-cli": "^1.1.13",
    "next": "^15.2.3",
    "next-sitemap": "^4.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.13",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^8",
    "eslint-config-next": "15.2.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
