{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "transpile": {
      "dependsOn": ["^transpile"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "build:clean": {
      "dependsOn": ["^build:clean"],
      "outputs": ["dist/**"]
    }
  }
}