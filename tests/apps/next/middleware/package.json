{
  "name": "gt-next-middleware-e2e",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:e2e": "node runPlaywright.mjs",
    "test:e2e:install": "npx playwright install chromium",
    "bench": "pnpm bench:unit && pnpm bench:e2e",
    "bench:unit": "vitest bench --run --config benchmarks/vitest.config.ts",
    "bench:e2e": "node runBenchE2E.mjs",
    "bench:unit:json": "vitest bench --run --config benchmarks/vitest.config.ts --outputJson benchmarks/results/unit-latest.json"
  },
  "dependencies": {
    "gt-next": "workspace:*",
    "next": "16.1.6",
    "react": "19.2.2",
    "react-dom": "19.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
