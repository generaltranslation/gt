{
  "name": "@generaltranslation/babel-plugin",
  "version": "0.0.0",
  "description": "A Babel plugin for compile-time optimization of GT translation components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "General Translation, Inc.",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
